{
  "expo": {
    "name": "DynastyMobileApp",
    "slug": "dynastymobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./apps/mobile/assets/images/icon.png",
    "splash": {
      "image": "./apps/mobile/assets/images/dynastysplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "entryPoint": "./apps/mobile/index.js",
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.mydynastyapp.dynasty",
      "googleServicesFile": "./apps/mobile/GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./apps/mobile/google-services.json",
      "package": "com.mydynastyapp.dynasty"
    },
    "extra": {
      "eas": {
        "projectId": "2a7efbb3-1b39-40e0-ae26-393387816d0e"
      }
    }
  }
}