{
  "name": "functions",
  "version": "1.0.0",
  "scripts": {
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint --config .eslintrc.js --ext .js,.ts .",
    "lint:ci": "eslint --ext .js,.ts src --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "build": "node esbuild.config.mjs",
    "build:watch": "node -e \"import('./esbuild.config.mjs').then(m => require('esbuild').context(m.buildOptions).then(ctx => ctx.watch()))\"",
    "build:prod": "cross-env NODE_ENV=production node esbuild.config.mjs",
    "build:analyze": "cross-env ESBUILD_ANALYZE=true node esbuild.config.mjs",
    "build:legacy": "tsc",
    "serve": "yarn build && firebase emulators:start --import=../emulator-data --export-on-exit",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "cross-env NODE_ENV=development yarn serve",
    "prod": "cross-env NODE_ENV=production yarn serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate:subscription-fields:test": "yarn dlx ts-node src/test/testSubscriptionFieldsMigration.ts",
    "migrate:subscription-fields:dry": "./scripts/run-subscription-fields-migration.sh dry-run",
    "migrate:subscription-fields:execute": "./scripts/run-subscription-fields-migration.sh execute",
    "migrate:subscription-fields:check": "./scripts/run-subscription-fields-migration.sh check"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@aws-sdk/client-pinpoint-sms-voice-v2": "^3.817.0",
    "@aws-sdk/client-s3": "^3.817.0",
    "@aws-sdk/client-ses": "^3.826.0",
    "@aws-sdk/client-sns": "^3.817.0",
    "@aws-sdk/s3-request-presigner": "^3.828.0",
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "@notionhq/client": "^3.1.3",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.0.1",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.15",
    "stripe": "^18.2.1",
    "winston": "^3.16.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.20.1",
    "@eslint/object-schema": "^2.1.6",
    "@jest/globals": "^30.0.0",
    "@types/jest": "^29.5.14",
    "@types/stripe": "^8.0.416",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^7.1.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.1",
    "firebase-functions-test": "^3.4.1",
    "glob": "^11.0.3",
    "jest": "^30.0.0",
    "lru-cache": "^11.1.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  },
  "private": true
}
