{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:ci": "eslint --ext .js,.ts src --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --import=../emulator-data --export-on-exit",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "cross-env NODE_ENV=development npm run serve",
    "prod": "cross-env NODE_ENV=production npm run serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate:subscription-fields:test": "npx ts-node src/test/testSubscriptionFieldsMigration.ts",
    "migrate:subscription-fields:dry": "./scripts/run-subscription-fields-migration.sh dry-run",
    "migrate:subscription-fields:execute": "./scripts/run-subscription-fields-migration.sh execute",
    "migrate:subscription-fields:check": "./scripts/run-subscription-fields-migration.sh check"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@aws-sdk/client-ses": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.817.0",
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "@notionhq/client": "^2.3.0",
    "@types/dotenv": "^6.1.1",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.9",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "libsodium-wrappers": "^0.7.15",
    "stripe": "^18.2.1",
    "twilio": "^5.6.1",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.19.2",
    "@eslint/object-schema": "^2.1.6",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/stripe": "^8.0.416",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.1",
    "firebase-functions-test": "^3.1.0",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "lru-cache": "^11.0.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "typescript": "^4.9.0"
  },
  "private": true
}
