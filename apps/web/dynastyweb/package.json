{
  "name": "dynastyweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "SENTRY_SUPPRESS_TURBOPACK_WARNING=1 next dev --turbopack",
    "dev:webpack": "next dev",
    "dev:prod": "NODE_ENV=production next dev --turbopack",
    "dev:emulator": "concurrently \"cd ../../firebase/functions && npm run build:watch\" \"cd ../../firebase && firebase emulators:start --import=./emulator-data --export-on-exit\" \"next dev --turbopack\" --rules",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:ci": "next lint --dir src/app --dir src/components --dir src/services --dir src/hooks --dir src/lib --dir src/context --dir src/utils",
    "test": "TEST_TYPE=unit jest",
    "test:watch": "TEST_TYPE=unit jest --watch",
    "test:coverage": "TEST_TYPE=unit jest --coverage",
    "test:integration": "TEST_TYPE=integration jest --config=jest.integration.config.js",
    "test:integration:watch": "TEST_TYPE=integration jest --config=jest.integration.config.js --watch",
    "test:all": "npm run test && npm run test:integration",
    "test:emulator": "USE_FIREBASE_EMULATOR=true jest",
    "test:emulator:watch": "USE_FIREBASE_EMULATOR=true jest --watch",
    "emulator:export": "cd ../../firebase && firebase emulators:export ./emulator-data",
    "emulator:start": "cd ../../firebase && firebase emulators:start --import=./emulator-data --export-on-exit"
  },
